<Window x:Class="MarketGame.MoveDrugsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarketGame"
        mc:Ignorable="d"
        Title="MoveDrugsWindow" Height="450" Width="800"
        Background="#FF262B37">
    <Grid>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="10,355,0,0" VerticalAlignment="Top" Height="30" Width="81" Click="BackButton_Click"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="37,25,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Downers.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="37,71,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Weed.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="37,117,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Acid.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="37,163,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Ecstacy.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="37,209,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Heroin.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="37,255,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Coke.png"/>

        <!-- This is hell -->
        <!-- Downers -->
        <Button Content="&lt;-" HorizontalAlignment="Left" Margin="115,38,0,0" VerticalAlignment="Top" Height="21" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="140,38,0,0" VerticalAlignment="Top" Height="21" Width="25"/>
        <TextBox HorizontalAlignment="Left" Margin="165,38,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Height="21" HorizontalContentAlignment="Center"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="285,38,0,0" VerticalAlignment="Top" Height="21" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="310,38,0,0" VerticalAlignment="Top" Height="21" Width="25"/>
        
        <!-- Weed -->
        <Button Content="Button" HorizontalAlignment="Left" Margin="140,84,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="285,84,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="115,84,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <TextBox HorizontalAlignment="Left" Margin="165,84,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Height="20" HorizontalContentAlignment="Center"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="310,84,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        
        <!-- Acid -->
        <Button Content="Button" HorizontalAlignment="Left" Margin="140,130,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="285,130,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="115,130,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <TextBox HorizontalAlignment="Left" Margin="165,130,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Height="20" HorizontalContentAlignment="Center"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="310,130,0,0" VerticalAlignment="Top" Height="20" Width="25"/>

        <!-- Ecstacy -->
        <Button Content="Button" HorizontalAlignment="Left" Margin="140,176,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="285,176,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="115,176,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <TextBox HorizontalAlignment="Left" Margin="165,176,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Height="20" HorizontalContentAlignment="Center"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="310,176,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        
        <!-- Heroin -->
        <Button Content="Button" HorizontalAlignment="Left" Margin="140,217,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="285,217,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="115,217,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <TextBox HorizontalAlignment="Left" Margin="165,217,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Height="20" HorizontalContentAlignment="Center"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="310,217,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        
        <!-- Coke -->
        <Button Content="Button" HorizontalAlignment="Left" Margin="140,263,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="285,263,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="115,263,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        <TextBox HorizontalAlignment="Left" Margin="165,263,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120" Height="20" HorizontalContentAlignment="Center"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="310,263,0,0" VerticalAlignment="Top" Height="20" Width="25"/>
        
        <Image HorizontalAlignment="Left" Height="46" Margin="366,25,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Downers.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="366,71,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Weed.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="366,117,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Acid.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="366,163,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Ecstacy.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="367,209,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Heroin.png"/>
        <Image HorizontalAlignment="Left" Height="46" Margin="366,250,0,0" VerticalAlignment="Top" Width="50" Source="/Icons/Coke.png"/>
    </Grid>
</Window>
